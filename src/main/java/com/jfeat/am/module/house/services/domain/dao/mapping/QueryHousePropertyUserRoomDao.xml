<mapper namespace="com.jfeat.am.module.house.services.domain.dao.QueryHousePropertyUserRoomDao">
    <sql id="Base_Column_List">
        t_house_property_user_room.id
        , t_house_property_user_room.user_id AS userId, t_house_property_user_room.room_id AS roomId
    </sql>


    <select id="queryMasterModel" resultType="HousePropertyUserRoomModel">
        SELECT t_house_property_user_room.*
        FROM t_house_property_user_room
        WHERE t_house_property_user_room.id = #{id}
        GROUP BY t_house_property_user_room.id
    </select>

    <select id="queryUserRoomByUserId" resultType="HousePropertyUserRoom">
        SELECT t_house_property_user_room.*,t_house_property_community.id AS communityId,t_house_property_community.community As communityName,
               t_house_property_building.id As buildingId,t_house_property_building.area AS buildingArea,t_house_property_building.code AS buildingCode,
               t_house_property_building_unit.id AS unitId,t_house_property_building_unit.area AS unitArea,t_house_property_building_unit.house_type AS houseType,
               t_house_property_room.number AS roomNumber
        FROM t_house_property_user_room
                          JOIN t_house_property_room ON t_house_property_room.id = t_house_property_user_room.room_id
                          JOIN t_house_property_building ON t_house_property_building.id = t_house_property_room.building_id
                          JOIN t_house_property_community ON  t_house_property_community.id=t_house_property_building.community_id
                          JOIN t_house_property_building_unit ON t_house_property_building_unit.id = t_house_property_room.unit_id
        WHERE t_house_property_user_room.user_id=#{userId};
    </select>
    <select id="findHousePropertyUserRoomPage" resultType="HousePropertyUserRoomRecord"
            parameterType="HousePropertyUserRoomRecord">
        SELECT
        <include refid="Base_Column_List"/>


        FROM t_house_property_user_room


        WHERE 1=1


        <if test="record.id != null and record.id>0 ">
            AND t_house_property_user_room.id LIKE CONCAT('%',#{record.id},'%')
        </if>


        <if test="record.userId != null and record.userId>0 ">
            AND t_house_property_user_room.user_id LIKE CONCAT('%',#{record.userId},'%')
        </if>


        <if test="record.roomId != null and record.roomId>0 ">
            AND t_house_property_user_room.room_id LIKE CONCAT('%',#{record.roomId},'%')
        </if>

        <if test="startTime != null">
            <![CDATA[AND t_house_property_user_room.start_time >= DATE(#{startTime})]]>
        </if>
        <if test="endTime != null">
            <![CDATA[AND t_house_property_user_room.end_time <= DATE(#{endTime}]]>
        </if>
        <!--
    <if test="search != null and search != ''">
        OR t_house_property_user_room.name LIKE CONCAT('%',#{search},'%')
    </if>
    -->
    </select>


