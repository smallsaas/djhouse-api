<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jfeat.am.module.house.services.domain.dao.QueryHouseFloorExchangeRequestDao">
                                                                                                                                                                                                                                                                                                                            <sql id="Base_Column_List">
                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                t_house_floor_exchange_request.id, t_house_floor_exchange_request.user_id AS userId, t_house_floor_exchange_request.asset_id AS assetId, t_house_floor_exchange_request.floor, t_house_floor_exchange_request.floor_state AS floorState, t_house_floor_exchange_request.stute, t_house_floor_exchange_request.create_time AS createTime, t_house_floor_exchange_request.update_time AS updateTime
    </sql>

    
    
    <select id="queryMasterModel" resultType="HouseFloorExchangeRequestModel">
       SELECT t_house_floor_exchange_request.*
       FROM t_house_floor_exchange_request
       WHERE t_house_floor_exchange_request.id=#{id}
       GROUP BY t_house_floor_exchange_request.id
    </select>



                                
    <select id="findHouseFloorExchangeRequestPage" resultType="HouseFloorExchangeRequestRecord" parameterType="HouseFloorExchangeRequestRecord">
            SELECT <include refid="Base_Column_List"/>

        

        FROM t_house_floor_exchange_request

        
        
        WHERE 1=1
        

        
                                                                            <if test="record.id != null and record.id>0 ">
                                    AND t_house_floor_exchange_request.id LIKE CONCAT('%',#{record.id},'%')
                            </if>
                                                                    

                                                                                        <if test="record.userId != null and record.userId>0 ">
                                    AND t_house_floor_exchange_request.user_id LIKE CONCAT('%',#{record.userId},'%')
                            </if>
                                                                    

                                                                                        <if test="record.assetId != null and record.assetId>0 ">
                                    AND t_house_floor_exchange_request.asset_id LIKE CONCAT('%',#{record.assetId},'%')
                            </if>
                                                                    <if test="record.floor != null and record.floor>0 ">
                                    AND t_house_floor_exchange_request.floor LIKE CONCAT('%',#{record.floor},'%')
                            </if>
                                                                    

                                                                                        <if test="record.floorState != null and record.floorState>0 ">
                                    AND t_house_floor_exchange_request.floor_state LIKE CONCAT('%',#{record.floorState},'%')
                            </if>
                                                                    <if test="record.stute != null and record.stute>0 ">
                                    AND t_house_floor_exchange_request.stute LIKE CONCAT('%',#{record.stute},'%')
                            </if>
                                                                                

                                                                                        <if test="record.createTime != null and record.createTime>0 ">
                                    AND t_house_floor_exchange_request.create_time LIKE CONCAT('%',#{record.createTime},'%')
                            </if>
                                                                                

                                                                                        <if test="record.updateTime != null and record.updateTime>0 ">
                                    AND t_house_floor_exchange_request.update_time LIKE CONCAT('%',#{record.updateTime},'%')
                            </if>
        
        <if test="startTime != null">
            <![CDATA[AND t_house_floor_exchange_request.start_time >= DATE(#{startTime})]]>
        </if>
        <if test="endTime != null">
            <![CDATA[AND t_house_floor_exchange_request.end_time <= DATE(#{endTime}]]>
        </if>
            <!--
        <if test="search != null and search != ''">
            OR t_house_floor_exchange_request.name LIKE CONCAT('%',#{search},'%')
        </if>
        -->
                            ORDER BY update_time DESC
                                                    , create_time DESC
                        </select>
</mapper>